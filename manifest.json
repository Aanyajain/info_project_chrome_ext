{
    "manifest_version": 2,
    "name": "HelloAST",
    "version": "1.0",
    "description": "my first extension",
    "icons": {
        "128": "i16.jpg",
        "48": "i16.jpg",
        "16": "i16.jpg"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "https://www.whoisxmlapi.com/*", 
        "chrome://favicon/"
    ],
    
    "browser_action": {
        "default_icon": "i16.jpg",
        "default_popup": "popup.html"
    }
}